{
	"id": "user",
	"type": "object",
	"properties": {
		"id": {
			"type": "string",
			"title": "User ID",
			"pattern": "^[a-zA-Z0-9_\\-]+$"
		},
		"type": {
			"type": "string",
			"title": "Type",
			"enum": ["human", "api"]
		},
		"profile": {
			"$ref": "profile",
			"title": "Profile"
		},
		"team_id": {
			"type": ["null", "string"],
			"title": "Team ID",
			"default": null
		},
		"status": {
			"type": "string",
			"title": "Status",
			"enum": ["active", "disabled"],
			"default": "active"
		}
	},
	"additionalProperties": false,
	"required": ["type", "profile"]
}
