{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": [0],
    "@typescript-eslint/no-use-before-define": [0]
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ]
}